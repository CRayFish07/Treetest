<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.gao.tree"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
   	<!-- 在SDCard中创建与删除文件权限 -->
	<uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
		<!-- 往SDCard写入数据权限 -->
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
	<uses-permission android:name="android.permission.RESTART_PACKAGES"/>
    <application android:icon="@drawable/icon" android:label="@string/app_name">
       <activity
            android:label="@string/app_name"
            android:name=".LoginActivity" 
            android:configChanges="orientation|keyboardHidden">
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity"  android:configChanges="orientation|keyboardHidden"></activity>
        <activity android:name=".TreeView"          android:configChanges="orientation|keyboardHidden"></activity>
        <activity android:name=".ListTableView" 	android:configChanges="orientation|keyboardHidden"></activity>
        <activity android:name=".ListTableView1" 	android:configChanges="orientation|keyboardHidden"></activity>
        <activity android:name=".ModifyActivity" 	android:configChanges="orientation|keyboardHidden"></activity>
        <activity android:name=".UserListView"   	android:configChanges="orientation|keyboardHidden"></activity>
        <activity android:name=".DictionaryActivity" android:configChanges="orientation|keyboardHidden"></activity>
        

    </application>
    <uses-sdk android:minSdkVersion="7" />

</manifest> 